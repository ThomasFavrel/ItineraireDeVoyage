  "rdfs:comment": {
    "de": [
      "Bistronomisches Restaurant mit Meerblick, in der Nähe des Hafens von Porto-Vecchio."
    ],
    "en": [
      "Bistronomic restaurant, sea view, close to the port of Porto-Vecchio."
    ],
    "it": [
      "Ristorante bistronomico, vista mare, vicino al porto di Porto-Vecchio."
    ],
    "fr": [
      "Restaurant bistronomique, vue mer, proche du port de Porto-Vecchio. Ouvert midi et soir. Carte en fonction des saisons, à base de produits frais. Spécialiste du tartare de poisson et de viande et un grand choix de moules frites. Parking gratuit."
    ],
    "es": [
      "Restaurante bistronómico, con vistas al mar, cerca del puerto de Porto-Vecchio. Abierto para el almuerzo y la cena. Menú de temporada basado en productos frescos. Especialistas en tartar de pescado y carne y una amplia oferta de mejillones fritos. Aparcamiento gratuito."
    ],
    "nl": [
      "Bistronomisch restaurant, uitzicht op zee, dicht bij de haven van Porto-Vecchio."
    ]
  },
