  "rdfs:comment": {
    "de": [
      "Restaurant mit Panoramablick aufs Meer, ein Ort zum Leben und zum Austausch. Moment der Flucht und Entspannung."
    ],
    "en": [
      "Restaurant with panoramic sea view, a place to live and exchange. Moment of escape and relaxation."
    ],
    "it": [
      "Ristorante con vista panoramica sul mare, un luogo dove vivere e scambiarsi. Momento di fuga e di relax."
    ],
    "fr": [
      "Cuisine méditerranéenne à partir de produits frais et locaux, grill viande et pêche locale, sélection de vins corses et champagne. Service midi et soir. Bar-piscine de 10h à minuit et piscine chauffée réservée à notre clientèle. Soirées musicales corses."
    ],
    "nl": [
      "Restaurant met panoramisch uitzicht op zee, een plek om te wonen en te ruilen. Moment van ontsnapping en ontspanning."
    ],
    "es": [
      "Cocina mediterránea con productos frescos de la zona, carnes y pescados locales a la parrilla, selección de vinos corsos y champán. Servicio de comidas y cenas. Bar de la piscina de 10 a 24 horas y piscina climatizada reservada para nuestros huéspedes. Veladas musicales corsas."
    ]
  },
